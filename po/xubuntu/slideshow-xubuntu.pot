msgid "Firefox Web Browser"
msgstr ""

#. type: Content of: <div><div><p>
#: slideshows/xubuntu/slides/04_defaultapps.html:12
msgid "Thunderbird Mail/News"
msgstr ""

#. type: Content of: <div><div><p>
#: slideshows/xubuntu/slides/04_defaultapps.html:13
msgid "Pidgin Internet Messenger"
msgstr ""

