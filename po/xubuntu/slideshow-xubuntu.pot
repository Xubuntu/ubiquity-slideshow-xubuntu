#. type: Content of: <div><div><p>
#: slideshows/xubuntu/slides/04_defaultapps.html:15
msgid "<b>Abiword</b>, word processor"
msgstr ""

#. type: Content of: <div><div><p>
#: slideshows/xubuntu/slides/04_defaultapps.html:16
msgid "<b>Gnumeric</b>, spreadsheet editor"
msgstr ""

#. type: Content of: <div><div><h2>
#: slideshows/xubuntu/slides/04_defaultapps.html:19
msgid "Media"
msgstr ""

#. type: Content of: <div><div><p>
