#~ "متى شئت."

#~ msgid "Frozen Bubble"
#~ msgstr "الفقاعة المتجمدة"

#~ msgid "Stellarium"
#~ msgstr "ستيلاريوم"

#~ msgid "Inkscape"
#~ msgstr "انكسكيب"

